       <property name="styleSheet">
        <string notr="true">/* QTableView Styling */
QTableView {
    background-color: #3d3d3d;  /* Dark background */
    color: #ffffff;  /* White text */
    border: 1px solid #00ff00;  /* Green border */
    border-radius: 8px;  /* Rounded corners */
    gridline-color: #00ff00;  /* Green gridlines */
    selection-background-color: #2d89ef;  /* Bright blue background when selected */
    selection-color: #ffffff;  /* White text when selected */
    alternate-background-color: #2b2b2b;  /* Alternate row background */
    padding: 8px;  /* Increased padding */
}

/* Header Styling for Horizontal Headers */
QHeaderView::section {
    background-color: #3d3d3d;  /* Dark background for header */
    color: #ffffff;  /* White text for headers */
    border-style: solid;
    border-width: 2px;
    border-color: #00ff00;  /* Green border */
    padding: 8px;
    font-weight: bold;  /* Bold header text */
    font-size: 12pt;
}

/* Vertical Header (Row Number Bar) Styling */
QHeaderView::section:vertical {
    background-color: #3d3d3d;  /* Dark background for row number bar */
    color: #ffffff;  /* White text for row numbers */
    border-style: solid;
    border-width: 2px;
    border-color: #00ff00;  /* Green border */
    padding: 8px;  /* Increase padding for row number cells */
    font-weight: bold;
    font-size: 10pt;
    min-width: 45px;  /* Increase width of row number bar */
}

/* Hover effect for header */
QHeaderView::section:hover {
    background-color: #2d89ef;  /* Blue background on hover */
    color: #ffffff;  /* Keep white text */
}

/* Row Styling */
QTableView::item {
    background-color: #333333;  /* Consistent background for rows */
    color: #ffffff;  /* White text */
    padding: 6px;  /* Increased padding for readability */
}

/* Even rows */
QTableView::item:alternate {
    background-color: #3d3d3d;  /* Slightly lighter for alternate rows */
}

/* Hover effect for rows */
QTableView::item:hover {
    background-color: #505050;  /* Lighter gray on hover */
    color: #ffffff;  /* Keep text white */
    border: 1px solid #00ff00;  /* Green border on hover */
}

/* Selected row with clearer styling */
QTableView::item:selected {
    background-color: #2d89ef;  /* Bright blue background for selected rows */
    color: #ffffff;  /* White text */
    border: 2px solid #00ff00;  /* Thicker green border around selected row */
    padding: 6px;  /* Ensure padding to avoid text clipping */
}
