   </property>
   <property name="styleSheet">
    <string notr="true">/* QComboBox Styling */
QComboBox {
    border-style: solid;
    background-color: #3d3d3d;  /* Same dark background */
    color: #fff;  /* White text */
    border-radius: 7px;  /* Rounded corners */
    border-width: 2px;  /* Set border width */
    border-color: #00ff00;  /* Green border */
    padding: 6px;  /* Padding inside the combo box */
    outline: none;  /* Remove default focus outline */
    qproperty-alignment: 'AlignVCenter | AlignHCenter';  /* Align text to the center */
    combobox-popup: 0;  /* Prevents the popup from covering the widget */
}

/* Drop-down button in QComboBox */
QComboBox::drop-down {
    width: 16px;  /* Width of the drop-down arrow button */
    background-color: #3d3d3d;  /* Same background color */
    border: none;  /* Remove borders from the button */
    subcontrol-position: right;  /* Position it on the right side */
    subcontrol-origin: padding;
}

/* Drop-down arrow */
QComboBox::down-arrow {
    width: 8px;  /* Fix arrow size */
    height: 8px;
    border: none;
    color: #fff;  /* White arrow */
}

QComboBox::down-arrow:hover {
    background-color: #2d2d2d;  /* Darker background on hover */
}

/* Hover effect */
QComboBox:hover {
