   <property name="styleSheet">
    <string notr="true">/* QLineEdit Styling */
QLineEdit {
    border-style: solid;
    background-color: #3d3d3d;  /* Same dark background as QPushButton */
    color: #fff;  /* White text */
    border-radius: 7px;  /* Rounded corners */
    border-width: 2px;  /* Set border width */
    border-color: #00ff00;  /* Green border */
    padding: 6px;  /* Padding inside the line edit */
    outline: none; /* Remove default focus outline */
    selection-background-color: #2d89ef; /* Blue selection background */
    selection-color: #fff; /* White selection text */
}

/* Hover effect */
QLineEdit:hover {
    background-color: qlineargradient(spread:pad, x1:0.517, y1:0, x2:0.517, y2:1, stop:0 rgba(45, 45, 45, 255), stop:1 rgba(29, 29, 29, 255));  /* Darker background on hover */
    border-color: #2d89ef;  /* Blue border on hover */
}

/* Focused effect (when the user clicks into the QLineEdit) */
QLineEdit:focus {
    background-color: qlineargradient(spread:pad, x1:0.517, y1:0, x2:0.517, y2:1, stop:0 rgba(29, 29, 29, 255), stop:1 rgba(45, 45, 45, 255));  /* Slightly darker background when focused */
    border-color: #da532c;  /* Red-orange border on focus */
}

/* Disabled QLineEdit */
QLineEdit:disabled {
    background-color: #2b2b2b;  /* Darker background for disabled state */
    color: #aaaaaa;  /* Gray text when disabled */
    border-color: #555555;  /* Lighter border when disabled */
}
</string>
   </property>
  </widget>
  <widget class="QDateEdit" name="hireDateEdit">
