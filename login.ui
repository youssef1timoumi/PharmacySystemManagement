     <property name="styleSheet">
      <string notr="true">/* QCheckBox Styling */
QCheckBox {
    background-color: #3d3d3d;  /* Dark background */
    color: #fff;  /* White text */
    border-radius: 7px;  /* Rounded corners */
    border: 2px solid #00ff00;  /* Green border */
    padding: 5px;  /* Padding around the checkbox text */
    margin: 3px;  /* Margin around the checkbox */
}

/* QCheckBox Indicator Styling (Checkbox Square) */
QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border-radius: 4px;  /* Slightly rounded indicator */
    background-color: #3d3d3d;  /* Match background */
    border: 2px solid #00ff00;  /* Green border around the checkbox */
}

/* QCheckBox Indicator When Checked */
QCheckBox::indicator:checked {
    background-color: #00ff00;  /* Green background when checked */
    border: 2px solid #00ff00;  /* Green border when checked */
}

/* QCheckBox Indicator When Hovered */
QCheckBox::indicator:hover {
    background-color: #4caf50;  /* Lighter green when hovered */
    border: 2px solid #4caf50;  /* Lighter green border on hover */
}

/* QCheckBox Disabled Styling */
QCheckBox:disabled {
    color: #666;  /* Greyed-out text */
    border-color: #666;  /* Greyed-out border */
}
